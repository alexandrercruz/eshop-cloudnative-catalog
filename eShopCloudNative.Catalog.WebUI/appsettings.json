{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Enterprise": {
        "Application": {
            "Log": {
                "Hostnames": [ "rabbitmq" ],
                "Port": 5672,
                "VHost": "EnterpriseApplicationLog",
                "Username": "log_user",
                "Password": "log_pass",
                "Exchange": "log_exchange",
                "ExchangeType": "fanout",
                "RouteKey": "",
                "DeliveryMode": 2
            }
        }
    }
}
